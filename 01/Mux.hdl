/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=nSel);
    And(a=a,b=nSel,out=aAndNsel);
    And(a=b,b=sel,out=bAndSel);
    Or(a=aAndNsel,b=bAndSel,out=out);
}
